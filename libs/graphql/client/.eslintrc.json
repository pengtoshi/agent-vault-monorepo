{
  "extends": ["../../../.eslintrc.json"],
  "ignorePatterns": ["!**/*", "__generated__/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {
        "@nx/enforce-module-boundaries": 0,
        "no-param-reassign": 0,
        "consistent-return": 0
      }
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
